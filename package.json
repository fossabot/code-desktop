{
  "name": "code-desktop",
  "version": "0.1.0",
  "description": "Desktop application for Strawbees CODE",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run install-src",
    "install-src": "cd src && npm install",
    "move-ui-stage": "strawbees-desktop-packager-cpdir -s ./node_modules/@strawbees/code-ui/out/desktop_stage/ui -o ./src/ui",
    "move-ui-production": "strawbees-desktop-packager-cpdir -s ./node_modules/@strawbees/code-ui/out/desktop_production/ui -o ./src/ui",
    "bundle": "strawbees-desktop-packager-bundle",
    "package": "strawbees-desktop-packager-package",
    "build": "npm run bundle && npm run package",
    "start": "cd src && npm start"
  },
  "license": "MIT",
  "dependencies": {
    "@strawbees/desktop-packager": "git://github.com/strawbees/desktop-packager.git#feature/create-api",
    "@strawbees/code-ui": "git://github.com/strawbees/code-ui.git#develop"
  }
}
